{
  "name": "submitty/submitty",
  "config": {
    "platform": {
      "php": "7.2"
    }
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "require": {
    "twig/twig": "2.12.1",
    "ramsey/uuid": "3.8.0",
    "aptoma/twig-markdown": "3.2.0",
    "erusev/parsedown": "1.7.3",
    "lcobucci/jwt": "3.3.1",
    "symfony/routing": "4.3.5",
    "symfony/config": "4.3.5",
    "symfony/http-foundation": "4.3.5",
    "doctrine/annotations": "1.8.0",
    "doctrine/cache": "1.8.0",
    "php-ds/php-ds": "1.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "8.4.1",
    "submitty/php-codesniffer": "2.2.0",
    "dealerdirect/phpcodesniffer-composer-installer": "0.5.0",
    "php-mock/php-mock-phpunit": "2.5.0",
    "phpstan/phpstan": "0.11.19"
  }
}
